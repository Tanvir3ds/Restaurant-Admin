<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.restadmin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="Admin"
        android:roundIcon="@drawable/roundcardbackground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AdminShowFood"></activity>
        <activity android:name=".AdminDataInsertUpdateDelete" />
        <activity
            android:name=".AdminFoodAddActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name=".Admin_Login"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>